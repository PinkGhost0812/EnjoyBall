<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="nonetitle" parent="AppTheme">
        <item name="windowNoTitle">true</item>
    </style>

    <style name="pop_animation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/personalcenter_in</item>
        <item name="android:windowExitAnimation">@anim/personalcenter_out</item>
    </style>

</resources>
